<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Framework SmartFactory</title>
    <parser>
        <target>docs</target>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>logs\phpdoc-{DATE}.log</default>
            <errors>logs\phpdoc-{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean" /> 
    </transformations>
    <files>
        <directory>includes\SmartFactory</directory>
        <directory>classes\SmartFactory</directory>
   </files>
</phpdoc>